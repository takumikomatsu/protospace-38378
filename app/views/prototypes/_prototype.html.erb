<div class="card">
  <%= link_to (image_tag prototype.image, class: 'card_img' if prototype.image.attached?), prototype_path(prototype)  %>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title%>
    <p class="">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to prototype.user.name, user_path(prototype.user.id), class: :card__user %>
  </div>
</div>